platform :ios, '11.0'

target 'PlaybookAccessibility' do
  inhibit_all_warnings!
  use_modular_headers!
  use_frameworks!

  # Pods for PlaybookAccessibility
  pod 'AccessibilitySnapshot/Core', :path => '~/AccessibilitySnapshot'

end

target 'Playbook-Accessibility-Tests' do
  inhibit_all_warnings!
  use_modular_headers!
  use_frameworks!

  # Pods for PlaybookAccessibility
  pod 'AccessibilitySnapshot/Core', :path => '~/AccessibilitySnapshot'

end

install! 'cocoapods', generate_multiple_pod_projects: true, incremental_installation: true